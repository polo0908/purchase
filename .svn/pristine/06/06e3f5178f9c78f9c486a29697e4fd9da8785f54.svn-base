
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button,
input, textarea, th, td {
    margin:0;
    padding:0;
    box-sizing:border-box;
    font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,Noto Sans CJK SC,WenQuanYi Micro Hei,Arial;
font-size:12px;
}
h1, h2, h3, h4, h5, h6{ font-size:100%; }
 em,i{ font-style:normal; }
small{ font-size:12px; }
ul, ol { list-style:none; }
a { text-decoration:none; }
a:hover,a:focus { text-decoration:underline; }
fieldset, img,button,input{ border:0 none;outline-style:none;outline:none;}
textarea,button,input{outline:none;}
textarea{border:none;outline:none;resize:none;}
button, input, select, textarea { font-size:100%; }
table { border-collapse:collapse; border-spacing:0; } 
.clearfix:before,.clearfix:after{content:"";display:table;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;} 
.red{color:red;}
.person_set{width: 760px;margin:0 auto;margin-top: 50px;}
.person_set h3,.person_new_add h3{font-size:28px;font-weight:700;padding-bottom: 28px;}
.person_set h3 button{position:relative;top:0px;left:100px;font-size:20px;font-weight:700;padding: 3px 5px;}
.person_set td,.person_set th{text-align:center;}
.person_set button{background-color:transparent;border:1px solid #ddd;}
.person_set .table-bordered>tbody>tr>td, .person_set .table-bordered>thead>tr>th{font-size:16px;text-align:center;vertical-align:middle;}
.person_new_add{width:1200px;margin:0 auto;margin-top:50px;} 
.person_new_add .w350{width: 350px;}
.person_new_add .w850{width:850px;}
.person_new_add .form-group{position: relative;width: 100%;margin-bottom: 10px;}
.person_new_add .mb_35{margin-bottom: 35px;}
.person_new_add .form-group .tip{position:absolute;top:32px;left:70px;}
.person_new_add .div_l label{width:65px;font-size: 16px;}
.person_new_add .div_l input{height:28px;}
.person_new_add .div_l .state_label{font-size: 15px;font-weight: normal;}
.person_new_add .div_l input[name="state"]{margin-right:5px;width: 14px;height:14px;position: relative;top:2px;}
.person_new_add .group_top label{width:60px;position: relative;top:2px;}
.person_new_add .group_top label:first-child{width:150px;}
.person_new_add .group_top input{margin-right: 5px;position:relative;top:2px;}
.person_new_add .group-bottom td input{margin-right:5px;}
.person_new_add .table>tbody>tr>td{vertical-align: middle;text-align: center;}
.person_new_add .btns button{background-color:transparent;border:1px solid #ddd;font-size: 16px;}
.person_new_add table label{margin-bottom:0;}
.person_new_add table label input{position:relative;top:2px;}
.person_new_add h4{font-weight: 700;font-size:20px;padding-bottom:15px;margin-top: 8px;}
.person_new_add .div_r label{font-size:16px;font-weight: normal;}
.person_new_add .div_r input{width: 16px;height: 16px;}
.person_new_add .div_r th, .person_new_add .div_r td{font-size: 16px;text-align: center;}




